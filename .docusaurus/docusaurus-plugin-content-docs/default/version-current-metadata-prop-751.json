  "docs": {
    "bug": {
      "id": "bug",
      "title": "已知bug",
      "description": "这里有一些已经发现的bug，以及解决方案，已知bug将在下个版本解决。",
      "sidebar": "tutorialSidebar"
    },
    "feat/call_this_method": {
      "id": "feat/call_this_method",
      "title": "调用任意方法",
      "description": "Cool Request支持在SpringBoot运行时调用任意容器内实例的方法、同时也包括静态方法。",
      "sidebar": "tutorialSidebar"
    },
    "feat/custom_function": {
      "id": "feat/custom_function",
      "title": "自定义函数",
      "description": "Cool Request拥有最强大得函数支持系统，当HTTP参数想要使用动态参数时，可根据自己需求，编写自定义函数。",
      "sidebar": "tutorialSidebar"
    },
    "feat/request_by_reflex": {
      "id": "feat/request_by_reflex",
      "title": "发起反射请求",
      "description": "发射请求可绕过系统中的拦截器，在开发时，可直接使请求进入到Controller方法。",
      "sidebar": "tutorialSidebar"
    },
    "feat/run_collections": {
      "id": "feat/run_collections",
      "title": "运行API集合",
      "description": "运行API集合类似Postman中Run Collections功能，可一键触发N个API。",
      "sidebar": "tutorialSidebar"
    },
    "feat/spring_gateway": {
      "id": "feat/spring_gateway",
      "title": "Spring网关自动发现",
      "description": "Cool Request在网关程序启动后，会自动提取网关配置的路径，并添加到环境变量中，用户只需要根据实际修改配置即可。",
      "sidebar": "tutorialSidebar"
    },
    "feat/stress_test": {
      "id": "feat/stress_test",
      "title": "压力测试",
      "description": "压力测试提供了简单的并发请求，可选择调用次数以及线程数，暂无指标分析。",
      "sidebar": "tutorialSidebar"
    },
    "feat/test": {
      "id": "feat/test",
      "title": "快速测试代码",
      "description": "快速测试代码基于正在运行的SpringBoot程序，将当前代码自动编译后注入到SpringBoot程序中，使代码快速生效，从而无需启动SpringBoot。",
      "sidebar": "tutorialSidebar"
    },
    "feat/tools/tools": {
      "id": "feat/tools/tools",
      "title": "小工具开发",
      "description": "Cool Request在v2024.7.15中增加了小工具，并且广大用户可自行开发小工具，同时可安装官方所提供的工具库。",
      "sidebar": "tutorialSidebar"
    },
    "feat/trace": {
      "id": "feat/trace",
      "title": "统计方法耗时",
      "description": "Cool Request可以追踪任意方法(除java包外)，将自动根据API入口函数向下推测所有方法，并统计各个方法的耗时。",
      "sidebar": "tutorialSidebar"
    },
    "intro": {
      "id": "intro",
      "title": "Cool Request",
      "description": "Cool Request是一款基于 IntelliJ IDEA 的插件，旨在简化 Spring MVC(Boot) 项目中 API调试调用，提供了强大的参数推测能力、方法耗时追踪、运行时反射调用等能力。",
      "sidebar": "tutorialSidebar"
    },
    "plugin_share": {
