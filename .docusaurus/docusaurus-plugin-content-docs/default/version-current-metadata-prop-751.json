  "docs": {
    "intro": {
      "id": "intro",
      "title": "什么是Cool Request",
      "description": "Cool Request是一款基于 IntelliJ IDEA 的插件，旨在简化 Spring MVC(Boot) 项目中 API调试调用，提供了强大的参数推测能力、方法耗时追踪、运行时反射调用等能力。",
      "sidebar": "tutorialSidebar"
    },
    "three-part/apifox": {
      "id": "three-part/apifox",
      "title": "apifox",
      "description": "Apifox 是接口管理、开发、测试全流程集成工具，Cool Request可以一键导出API到Apifox中。",
      "sidebar": "tutorialSidebar"
    },
    "three-part/apipost": {
      "id": "three-part/apipost",
      "title": "apipost",
      "description": "Apipost 是接口管理、开发、测试全流程集成工具，Cool Request可以一键导出API到Apipost中。",
      "sidebar": "tutorialSidebar"
    },
    "tutorial-basics/api_search": {
      "id": "tutorial-basics/api_search",
